<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
   <Version>2.0.1-beta-020</Version>
    <Authors>zhenlei520</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>zhenlei520</Company>
    <Description>dotnet core baidu identification</Description>
    <RepositoryUrl>https://github.com/zhenlei520/System.Extension.Core</RepositoryUrl>
    <PackageProjectUrl>https://www.nuget.org/packages/EInfrastructure.Core.BaiDu.ContentIdentification/</PackageProjectUrl>
    <Copyright>Copyright ©zhenlei520 2020</Copyright>
    <PackageIconUrl>https://secure.gravatar.com/avatar/932ca0f8eaf4df1aaf221f9a1a897569?s=512</PackageIconUrl>
    <Title>DotnetCore压缩包类库</Title>
    <PackageTags>Compress ICSharpCode</PackageTags>
    <IsPackable>false</IsPackable>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <NeutralLanguage>zh-CN</NeutralLanguage>

  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\EInfrastructure.Core.BaiDu.ContentIdentification.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\EInfrastructure.Core.BaiDu.ContentIdentification.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Infrastructure\src\EInfrastructure.Core\EInfrastructure.Core.csproj"/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0"/>
    <PackageReference Include="RestSharp" Version="106.3.1"/>
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.1.0"/>
  </ItemGroup>

</Project>
