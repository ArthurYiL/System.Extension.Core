<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <Version>2.0.1-beta-008</Version>
    <Authors>zhenlei520</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>zhenlei520</Company>
    <Description>dotnet core 基础模型配置</Description>
    <RepositoryUrl>https://github.com/zhenlei520/System.Extension.Core</RepositoryUrl>
    <PackageProjectUrl>https://www.nuget.org/packages/EInfrastructure.Core.Tools/</PackageProjectUrl>
    <Copyright>Copyright ©zhenlei520 2020</Copyright>
    <PackageIconUrl>https://secure.gravatar.com/avatar/932ca0f8eaf4df1aaf221f9a1a897569?s=512</PackageIconUrl>
    <Title>基础模型配置</Title>
    <PackageTags>EInfrastructure.Core.Config.Entities EInfrastructure ECommon</PackageTags>
    <NeutralLanguage>zh-CN</NeutralLanguage>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\EInfrastructure.Core.Config.Entities.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\EInfrastructure.Core.Config.Entities.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\EInfrastructure.Core.Configuration\EInfrastructure.Core.Configuration.csproj" />
  </ItemGroup>

</Project>
